<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sittingtherestanding.ambition" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
	<name>Ambition</name>
	<description>
		Use math to reach the top of the world!
	</description>
	<author email="sittingtherestanding@gmail.com" href="http://www.sittingtherestanding.com">
		Sitting There Standing
	</author>

	<preference name="phonegap-version" value="3.5.0" />

	<preference name="fullscreen" value="true" />
	<preference name="orientation" value="portrait" />
	<preference name="disallowOverscroll" value="true" />
	<preference name="EnableViewportScale" value="true"/>
	<preference name="webviewbounce" value="false" />

	<icon src="icon.png" />

	<platform name="ios">
		<!-- iOS 7.0+ -->
		<!-- iPhone / iPod Touch  -->
		<icon src="distribution/icons/ios/Icon-60.png" width="60" height="60" />
		<icon src="distribution/icons/ios/Icon-120.png" width="120" height="120" />
		<!-- iPad -->
		<icon src="distribution/icons/ios/Icon-76.png" width="76" height="76" />
		<icon src="distribution/icons/ios/Icon-152.png" width="152" height="152" />
		<!-- iOS 6.1 -->
		<!-- Spotlight Icon -->
		<icon src="distribution/icons/ios/Icon-40.png" width="40" height="40" />
		<icon src="distribution/icons/ios/Icon-40@2x.png" width="80" height="80" />
		<!-- iPhone / iPod Touch -->
		<icon src="distribution/icons/ios/Icon.png" width="57" height="57" />
		<icon src="distribution/icons/ios/Icon@2x.png" width="114" height="114" />
		<!-- iPad -->
		<icon src="distribution/icons/ios/Icon-72.png" width="72" height="72" />
		<icon src="distribution/icons/ios/Icon-72@2x.png" width="144" height="144" />
		<!-- iPhone Spotlight and Settings Icon -->
		<icon src="distribution/icons/ios/Icon-Small.png" width="29" height="29" />
		<icon src="distribution/icons/ios/Icon-Small@2x.png" width="58" height="58" />
		<!-- iPad Spotlight and Settings Icon -->
		<icon src="distribution/icons/ios/Icon-Small-50.png" width="50" height="50" />
		<icon src="distribution/icons/ios/Icon-Small-50@2x.png" width="100" height="100" />
	</platform>

	<access origin="*" />
</widget>
